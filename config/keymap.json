[
  {
    "context": "EmptyPane || SharedScreen",
    "bindings": {
      ", e": "file_finder::Toggle",
      ", g": "project_symbols::Toggle",
      "t t": "file_finder::Toggle"
    }
  },
  {
    "context": "Editor && vim_mode == normal",
    "bindings": {
      ", e": "file_finder::Toggle",
      ", g": "project_symbols::Toggle",
      ", d": "editor::GoToDefinition",
      "t t": "file_finder::Toggle"
    }
  },
  {
    "bindings": {
      "alt-left": "pane::ActivatePreviousItem",
      "alt-right": "pane::ActivateNextItem",
      "ctrl-j": "terminal_panel::Toggle",
      "ctrl-i": "agent::ToggleFocus",
      "F2": "workspace::CloseAllDocks",
      "F3": "project_panel::ToggleFocus",
      "F4": "outline_panel::ToggleFocus"
    }
  }
]
